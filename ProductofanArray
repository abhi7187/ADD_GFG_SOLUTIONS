

//User function Template for Java

/*  
     arr[] is the array
     n = number of element in array
     mod = modulo value;
*/
class GfG
{
    public static Long product(Long arr[], Long mod, int n)
    {
        //yout code here
        long p=1;
        for(long l: arr){
            p=(p*l)%mod;
        }
        return p;
    }
}
